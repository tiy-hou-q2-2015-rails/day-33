<h1>Sessions#new</h1>

<%= form_tag auth_path, method: :post do %>
  <div class="form-group">
    <%= label_tag :username %>
    <%= text_field_tag :username, params[:username], class: "form-control" %>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, "", class: "form-control" %>
  </div>
  <div class="form-group">
    <%= submit_tag "Sign In", class: "btn btn-primary" %>
  </div>
<% end %>
